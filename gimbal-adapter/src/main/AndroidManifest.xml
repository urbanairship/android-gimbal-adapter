<manifest package="com.urbanairship.gimbal"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application>

        <receiver
            android:name="com.urbanairship.gimbal.AirshipReadyReceiver"
            android:exported="false">

            <intent-filter>
                <action android:name="com.urbanairship.AIRSHIP_READY"/>
                <category android:name="${applicationId}"/>
            </intent-filter>

        </receiver>

        <receiver android:name="com.urbanairship.gimbal.GimbalAirshipReceiver"
            android:exported="false">

            <intent-filter>
                <action android:name="com.urbanairship.push.CHANNEL_UPDATED" />
                <category android:name="${applicationId}" />
            </intent-filter>
        </receiver>


    </application>

</manifest>
